    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "deploy": "eas update --channel development",
    "deploy:android": "eas build --platform android --profile development --no-wait",
    "deploy:ios": "eas build --platform ios --profile development --no-wait",
    "submit:android": "eas submit --platform android --profile development --no-wait",
    "submit:ios": "eas submit --platform ios --profile development --no-wait",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:with:react-native:android": "expo build:android --no-publish --config app.json"
  },
